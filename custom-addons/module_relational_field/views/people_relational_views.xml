<odoo>
	<record id="people_view_form" model="ir.ui.view">
		<field name="name">people_relational.form</field>
		<field name="model">people_relational</field>
		<field name="arch" type="xml">
			<form string="People">
				<sheet>
					<group>
						<field name="name" />
						<notebook>
							<page string="House">
								<field name="house_ids" widget="many2many"/>
							</page>
						</notebook>
						<group></group>
					</group>
				</sheet>
			</form>
		</field>
	</record>

	<record id="people_action" model="ir.actions.act_window">
		<field name="name">People Relational</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">people_relational</field>
	</record>

	<menuitem id="menu_people" name="xxx2Many Field" parent="menu_field" action='people_action' />
</odoo>
